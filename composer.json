{
  "name": "tebe/html",
  "description": "Framework Agnostic HTML Helper Class for Creating HTML Tags and Form Elements.",
  "keywords": [
    "php",
    "html",
    "form"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Tebe\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "phpunit",
      "phpcs"
    ],
    "run": "php -S localhost:9999 -t example",
    "phpunit": "php vendor/bin/phpunit",
    "phpcs": "php vendor/bin/phpcs"
  }
}